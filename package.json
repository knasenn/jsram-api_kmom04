    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.19.0",
    "javascript-style-guide": "^0.9.10",
