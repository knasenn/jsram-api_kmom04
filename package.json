{
  "name": "me-api",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha 'test/reports/report_integration.js'",
    "test-scrutinizer": "nyc --all --reporter=clover mocha 'test/reports/report_integration.js'",
    "start": "node app.js",
    "stop": "taskkill -F -IM node.exe",
    "production": "NODE_ENV='production' node app.js",
    "productionz": "NODE_ENV='production' && node app.js",
    "eslint": "eslint .",
    "report-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "report-codecov": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "clean": "rm -rf npm-debug.log coverage .nyc_output",
    "preclean-all": "npm run clean",
    "clean-all": "rm -rf node_modules"
  },
